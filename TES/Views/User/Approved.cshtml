@model IEnumerable<TES.Models.Approved_View>

@{
    ViewBag.Title = "Approved";
    Layout = "~/Views/Shared/_Layout_User.cshtml";
}

<h5>Approved</h5>
<hr />
<table class="mdl-data-table" style="width:100%">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenderId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Datetime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenderId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Datetime)
        </td>
        <td class="select">
            @Html.ActionLink("Detay", "ApprovedDetail", new { id = item.Id })
        </td>
    </tr>
}

</table>
